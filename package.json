{
  "name": "world-metro-generator",
  "version": "1.0.0",
  "description": "A simple tool that programmatically generates a list of world metros based on a few simple metrics",
  "scripts": {
    "clean": "rm -rf temp",
    "cli": "npm run clean; ts-node --transpile-only src/cli.ts",
    "generate": "npm run clean; ts-node --transpile-only src/cli.ts generate",
    "export": "npm run clean; ts-node --transpile-only src/cli.ts export"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jgillick/world-metro-generator.git"
  },
  "keywords": [
    "geo",
    "metro",
    "postgres"
  ],
  "author": "Jeremy Gillick",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/jgillick/world-metro-generator/issues"
  },
  "homepage": "https://github.com/jgillick/world-metro-generator#readme",
  "devDependencies": {
    "@types/node": "^17.0.24",
    "@types/yargs": "^17.0.10"
  },
  "dependencies": {
    "JSONStream": "^1.3.5",
    "knex": "^1.0.7",
    "node-stream-zip": "^1.15.0",
    "pg": "^8.7.3",
    "ts-node": "^10.7.0",
    "typescript": "^4.6.3",
    "yargs": "^17.4.1"
  }
}
